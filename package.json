{
  "name": "mcp-servers",
  "version": "1.0.0",
  "description": "A collection of Minecraft Protocol (MCP) servers implemented in TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start:basic": "node dist/servers/basic/index.js",
    "start:auth": "node dist/servers/auth/index.js",
    "start:proxy": "node dist/servers/proxy/index.js",
    "dev:basic": "ts-node-dev --respawn src/servers/basic/index.ts",
    "dev:auth": "ts-node-dev --respawn src/servers/auth/index.ts",
    "dev:proxy": "ts-node-dev --respawn src/servers/proxy/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "minecraft",
    "protocol",
    "server",
    "typescript"
  ],
  "author": "pgtipping",
  "license": "MIT",
  "dependencies": {
    "minecraft-protocol": "^1.44.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}